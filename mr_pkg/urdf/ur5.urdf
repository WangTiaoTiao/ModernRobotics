<?xml version="1.0"?>
<robot name="ur5">

    <material name="blue">
        <color rgba="0.0 0.0 1.0 1.0"/>
    </material>

    <material name="white">
        <color rgba="1 1.0 1.0 1.0"/>
    </material>
    
    <material name="black">
        <color rgba="0 0.0 0.0 1.0"/>
    </material>

    <!-- <kinematic Joint -->
    <joint name="world_joint" type="fixed">
        <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
        <parent link="world"/>
        <child link="base_link"/>
    </joint>

    <joint name="joint1" type="continuous">
        <origin xyz="0.0 0.0 0.089159" rpy="0.0 0.0 0.0"/>
        <parent link="base_link"/>
        <child link="link1"/>
        <axis xyz="0.0 0.0 1.0"/>
    </joint>

    <joint name="joint2" type="continuous">
        <origin xyz="0.0 0.13585 0.0" rpy="0.0 1.57079 0.0"/>
        <parent link="link1"/>
        <child link="link2"/>
        <axis xyz="0.0 1.0 0.0"/>
    </joint>

    <joint name="joint3" type="continuous">
        <origin xyz="0.0 -0.1197 0.425" rpy="0.0 0.0 0.0"/>
        <parent link="link2"/>
        <child link="link3"/>
        <axis xyz="0.0 1.0 0.0"/>
    </joint>

    <!-- Link interia-->

    <link name="world"/>
    
    <link name="base_link">
        <inertial>
            <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
            <mass value="4.0"/>
            <inertia ixx="0.00443333156" ixy="0.0" ixz="0.0" 
                     iyy="0.00443333156" iyz="0.0" izz="0.0072"/>
        </inertial>
        <visual>
            <geometry>
                <box size="0.1 0.1 0.1"/>
            </geometry>
            <material name="black"/>
        </visual>
    </link>

    <link name="link1">
        <inertial>
            <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
            <mass value="3.7"/>
            <inertia ixx="0.010267495893" ixy="0.0" ixz="0.0" 
                     iyy="0.010267495893" iyz="0.0" izz="0.00666"/>
        </inertial>
        <visual>
            <geometry>
                <box size="0.1 0.1 0.1"/>
            </geometry>
            <material name="blue"/>
        </visual>
    </link>

    <link name="link2">
        <inertial>
            <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.28"/>
            <mass value="8.393"/>
            <inertia ixx="0.22689067591" ixy="0.0" ixz="0.0" 
                     iyy="0.22689067591" iyz="0.0" izz="0.0151074"/>
        </inertial>
        <visual>
            <geometry>
                <box size="0.1 0.1 0.1"/>
            </geometry>
            <material name="blue"/>
        </visual>
    </link>

    <link name="link3">
        <inertial>
            <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.25"/>
            <mass value="2.275"/>
            <inertia ixx="0.049443313556" ixy="0.0" ixz="0.0" 
                     iyy="0.049443313556" iyz="0.0" izz="0.004095"/>
        </inertial>
        <visual>
            <geometry>
                <box size="0.1 0.1 0.1"/>
            </geometry>
            <material name="blue"/>
        </visual>
    </link>



</robot>